
---------------------

[1,2,3].map(x=> x*2) //[2,4,6]
[1,2,3].reduce((x,y)=>x*y) //6
[1,21,132].sort() //[1,132,21]
[1,2,3].reverse()  //[3,2,1]

---------------------

['a','b'].concat(['v']) //['a','b','v']
['a','b'].join('-') //'a-b'
['a','b','c'].slice(1) //['b','c']

---------------------

const arr=['a','b','c','d'];
const haha=arr.splice(1,2,'f');
//arr=['a','f','d'] , haha=['b','c']


